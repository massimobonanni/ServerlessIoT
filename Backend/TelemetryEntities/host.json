{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingExcludedTypes": "Request",
      "samplingSettings": {
        "isEnabled": true
      }
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "SIOTHub"
    },
    "eventHubs": {
      "maxEventBatchSize": 10,
      "batchCheckpointFrequency": 1,
      "prefetchCount": 300,
      "initialOffsetOptions": {
        "type": "fromStart",
        "enqueuedTimeUtc": ""
      },
      "clientRetryOptions": {
        "mode": "exponential",
        "tryTimeout": "00:01:00",
        "delay": "00:00:00.80",
        "maxDelay": "00:01:00",
        "maxRetries": 3
      }
    }
  }
}